; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Barebone-Template"
config/auto_accept_quit=false
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AcousticAudioManager="*res://addons/acoustic/src/audio_manager.gd"
AcousticMusicManager="*res://addons/acoustic/src/music_manager.gd"
AcousticSoundPool="*res://addons/acoustic/src/sound_pool.gd"
VaultSaveManager="*res://addons/vault/src/save_manager.gd"
OmniKitWindowManager="*res://addons/omnikit/src/autoloads/viewport/window_manager.gd"
OmniKitLocalNetworkHandler="*res://addons/omnikit/src/autoloads/network/local_network_handler.gd"
OmniKitGamepadControllerManager="*res://addons/omnikit/src/autoloads/gamepad/gamepad_controller_manager.gd"
Globals="*res://autoload/general/global.gd"
GlobalEvents="*res://autoload/general/global_events.gd"
SettingsManager="*res://autoload/persistence/settings_manager.tscn"
GlobalEffects="*res://ui/effects/global_effects.tscn"
UiAnimations="*res://ui/animations/ui_animations.tscn"
Warp="*res://addons/warp/src/warp.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/acoustic/plugin.cfg", "res://addons/machina/plugin.cfg", "res://addons/omnikit/plugin.cfg", "res://addons/vault/plugin.cfg", "res://addons/warp/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "pink",
"res://autoload/": "yellow",
"res://components/": "red",
"res://localization/": "orange",
"res://shaders/": "purple",
"res://ui/": "green"
}

